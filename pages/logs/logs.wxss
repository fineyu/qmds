  flex-direction: column;
}
 
.sku-price {
  color: red;
  position: relative;
  left: 250rpx;
  margin: 1rpx;
}
.sku-title {
  position: relative;
  left: 250rpx;
  font-size: 12px;
  margin: 1rpx;
}
 
.line{
  width: 95%;
  height: 2rpx;
  display: inline-block;
  margin: 30rpx 0rpx 5rpx 0rpx;
  background-color: #f2f2f2;
  text-align: center;
}
 
.gg-title{
 font-size: 15px;
}
 
.gg text{
  padding: 7rpx;
  height:50rpx;
  line-height:50rpx;
