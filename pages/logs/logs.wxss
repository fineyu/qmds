}
.details{
  width: 100vw;
  height: 100rpx;
  line-height: 100rpx;
  font-family: "微软雅黑";
  font-size: 32rpx;
  color: #061010;
  background-color: #fff;
}
.details text{
    width: 50%;
    height: 100rpx;
    line-height: 100rpx;
    display: block;
    float: left;
    text-align: center;
}
.green{
  border-bottom: 2rpx solid #57c340;
  color: #57c340 !important;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
